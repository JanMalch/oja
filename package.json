{
  "name": "oja",
  "version": "0.0.1-alpha.2",
  "scripts": {
    "dev": "esbuild src/index.mjs --outdir=www --bundle --servedir=www",
    "build:js": "esbuild src/index.mjs --outdir=dist --bundle --minify",
    "build:css": "esbuild --bundle www/styles.css --minify --outfile=dist/styles.css",
    "build:html": "html-minifier-terser --collapse-whitespace --remove-comments -o dist/index.html www/index.html",
    "build": "npm run build:js && npm run build:css && npm run build:html"
  },
  "private": true,
  "author": "JanMalch",
  "license": "NONE",
  "description": "",
  "devDependencies": {
    "@types/js-yaml": "^4.0.9",
    "esbuild": "^0.22.0",
    "html-minifier-terser": "^7.2.0"
  },
  "dependencies": {
    "@openapi-contrib/openapi-schema-to-json-schema": "^5.1.0",
    "@segment/ajv-human-errors": "^2.13.0",
    "ajv": "^8.16.0",
    "ajv-draft-04": "^1.0.0",
    "js-yaml": "^4.1.0"
  }
}
